{
  "expo": {
    "owner": "fikfikriii",
    "name": "unistuff",
    "slug": "unistuff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-unistuff.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-unistuff.png",
      "resizeMode": "contain",
      "backgroundColor": "#005AAB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ppam.unistuff",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "packages": "com.ppam.unistuff",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-unistuff.png",
        "backgroundColor": "#005AAB"
      },
      "package": "com.ppam.unistuff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a13ba44d-f38e-4f64-a558-5c6f19c8d844"
      }
    }
  }
}
